{
  "version": "2.2.3",
  "license": "MIT",
  "name": "jbrowse-plugin-msaview",
  "keywords": [
    "jbrowse",
    "jbrowse2"
  ],
  "main": "dist/index.js",
  "files": [
    "dist",
    "src"
  ],
  "config": {
    "port": 9000,
    "browse": {
      "port": 8999
    },
    "jbrowse": {
      "plugin": {
        "name": "MsaView"
      }
    }
  },
  "scripts": {
    "clean": "rimraf dist",
    "start": "node esbuild-watch.mjs",
    "format": "prettier --write .",
    "build": "tsc && NODE_ENV=production node esbuild.mjs && cp distconfig.json dist/config.json",
    "prebuild": "yarn clean",
    "lint": "eslint --report-unused-disable-directives --max-warnings 0",
    "pretest": "test -d .test-jbrowse || jbrowse create .test-jbrowse --nightly",
    "test": "vitest run",
    "test:watch": "vitest",
    "prepack": "yarn build",
    "postversion": "git push --follow-tags"
  },
  "jbrowse-plugin": {
    "name": "MsaView"
  },
  "dependencies": {
    "@emotion/styled": "^11.14.1",
    "g2p_mapper": "^2.0.0",
    "pako": "^2.1.0",
    "react-msaview": "^4.8.1",
    "swr": "^2.3.8"
  },
  "devDependencies": {
    "@emotion/react": "^11.14.0",
    "@eslint/js": "^9.39.2",
    "@fal-works/esbuild-plugin-global-externals": "^2.1.2",
    "@jbrowse/core": "^3.7.0",
    "@jbrowse/plugin-linear-genome-view": "^3.7.0",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@mui/system": "^7.3.7",
    "@mui/x-data-grid": "^8.24.0",
    "@types/node": "^24.10.4",
    "@types/pako": "^2.0.4",
    "@types/react": "^19.2.7",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.26",
    "eslint-plugin-unicorn": "^62.0.0",
    "mobx": "^6.15.0",
    "mobx-react": "^9.2.1",
    "mobx-state-tree": "^5.4.2",
    "prettier": "^3.7.4",
    "pretty-bytes": "^7.1.0",
    "puppeteer": "^24.34.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "rimraf": "^6.1.2",
    "rxjs": "^7.8.2",
    "serve": "^14.2.5",
    "tss-react": "^4.9.20",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0",
    "vitest": "^4.0.16"
  }
}
